<p id="notice"><%= notice %></p>

<h1>Spree Packs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spree_packs.each do |spree_pack| %>
      <tr>
        <td><%= link_to 'Show', spree_pack %></td>
        <td><%= link_to 'Edit', edit_spree_pack_path(spree_pack) %></td>
        <td><%= link_to 'Destroy', spree_pack, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spree Pack', new_spree_pack_path %>
